	local bReadOnly = WindowManager.getReadOnlyState(getDatabaseNode()) or performed
	
	for i=1,7 do 
		self["trick_" .. i .. "_level"].setReadOnly(bReadOnly)
		self["trick_" .. i .. "_level"].onValueChanged = function()
			CircusManager.CalcShow(getDatabaseNode())
		end
		
		for j=1,3 do 
			self["trick_" .. i .. "_result_" .. j].setReadOnly(bReadOnly)
			self["trick_" .. i .. "_result_" .. j].onValueChanged = function()
				CircusManager.CalcShow(getDatabaseNode())
			end
		end
	end
	
	local fields = {
		"name", "circus", "linkCircus", "settlement", "advertisementTier", "anticipation",
		"maxAnt", "excitement", "promotion", "beer", "confections", "binos", "basicMerch",
		"qualityMerch", "extraordinraryMerch", "payout", "bad_weather"
	}
	
	for _, field in ipairs(fields) do 
		self[field].setReadOnly(bReadOnly)
	end
end</LUACode>
